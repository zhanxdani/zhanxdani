#include <stdio.h>

extern int tribonacci(long num);

int main() {
    long num;
    printf("Enter a number: ");
    scanf("%ld", &num);
    if (tribonacci(num)) {
        printf("%ld is a part of the Tribonacci sequence.\n", num);
    } else {
        printf("%ld is not a part of the Tribonacci sequence.\n", num);
    }
    return 0;
}
